{
  "name": "gqly",
  "version": "1.1.0",
  "description": "Bridge REST APIs and GraphQL by mapping existing controllers",
  "repository": {
    "type": "git",
    "url": "https://github.com/domz-1/gqly.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": "./dist/index.js",
    "./nestjs": "./dist/nestjs/gqly.module.js"
  },
  "workspaces": [
    "examples/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "example:express": "node examples/express/app.js",
    "example:nest": "npx ts-node examples/nest-app/src/main.ts"
  },
  "keywords": [
    "graphql",
    "rest",
    "express",
    "middleware",
    "adapter",
    "api",
    "bridge",
    "controller",
    "mapping",
    "rest-to-graphql",
    "auto-generation",
    "schema",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "graphql": "^16.8.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/platform-express": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "express": "^4.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/graphql": "^14.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "typescript": "^5.3.3"
  }
}